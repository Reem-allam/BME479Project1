
void keyPressed() {
  switch (key){
    case ENTER:
    screen = 1;
    break;
    
    case 'e':
     screen = 2;
     break;
     
    case 'b':
    screen = 1;
    break;
    
    case 'r':
    screen = 0;
    break;
    
    case 's':
    screen = 3;
    break;
  
    case 'o':
    screen = 3;
    break;
    
    case 'm':
    screen = 4;
    break; 
  }
}
